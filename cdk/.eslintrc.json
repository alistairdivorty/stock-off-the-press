{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": ["error", 4],
        "semi": [2, "always"],
        "space-before-function-paren": [0, "ignore"],
        "require-jsdoc": 0
    }
}
